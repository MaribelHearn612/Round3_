@model IEnumerable<Round3.TipoUsuario>

@{
    ViewBag.Title = "Index";
}

<h2>Tipo de usuario</h2>

<p>
    @Html.ActionLink("Crear", "Create")
</p>
<table class="table">
    <tr>
        <th>
            Tipo de usuario
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.nombreTipoU)
        </td>
        <td>
            @Html.ActionLink("Editar", "Edit", new { id=item.idTipoU }) |
            @Html.ActionLink("Detalles", "Details", new { id=item.idTipoU }) |
            @Html.ActionLink("Borrar", "Delete", new { id=item.idTipoU })
        </td>
    </tr>
}

</table>
